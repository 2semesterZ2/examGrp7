<header>
    <nav class="navbar fixed-top navbar-expand-xs navbar-dark bg-darkblue text-white">
        <div class="container">
            <% if(pageId !== null) { %>
            <h1 class="page-title font-weight-bold h6 mb-0"><%= pageName %></h1>
            <% } %>
            <div class="d-flex w-100 flex-row justify-content-between align-items-center">
                <a class="navbar-brand" href="/">
                    <img class="brand-logo d-none d-md-block" src="/img/unchained-logo.svg" alt="Unchained">
                    <img class="brand-logo d-block d-md-none" src="/img/unchained-logo-small.svg" alt="Unchained">
                </a>
                <a href="#" class="menu-toggle hover-primary">
                    <div class="off">
                        <span class="font-weight-bold d-none d-md-inline-block mr-3">MENU</span>
                        <span class="h2 m-0 d-inline-block"><i class="fas fa-bars"></i></span>
                    </div>
                    <div class="on">
                        <span class="font-weight-bold d-none d-md-inline-block mr-3">CLOSE</span>
                        <span class="h2 m-0 d-inline-block"><i class="fas fa-times"></i></span>
                    </div>
                </a>
            </div>
        </div>
    </nav>
</header>
